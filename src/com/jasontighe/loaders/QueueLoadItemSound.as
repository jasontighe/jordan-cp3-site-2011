package com.jasontighe.loaders {	import flash.media.Sound;	import flash.media.SoundLoaderContext;		import com.jasontighe.loaders.AbstractQueueLoadItem;		public class QueueLoadItemSound extends AbstractQueueLoadItem	{		public function QueueLoadItemSound( url:String )		{						super( url );						_context = getDefaultContext( );			_loadItem = new Sound( );			_loadListener = _loadItem;		}						private function getDefaultContext( ):SoundLoaderContext		{			var context:SoundLoaderContext = new SoundLoaderContext( );			return context;		}	}}