package com.jasontighe.loaders {	import flash.display.Loader;	import flash.system.ApplicationDomain;	import flash.system.LoaderContext;	import com.jasontighe.loaders.AbstractQueueLoadItem;		public class QueueLoadItemDisplay extends AbstractQueueLoadItem	{		public function QueueLoadItemDisplay( url:String  )		{										super( url );						_context = getDefaultContext( );			_loadItem = new Loader( );			_loadListener = Loader( _loadItem ).contentLoaderInfo;		}									private function getDefaultContext( ):LoaderContext		{			var context:LoaderContext = new LoaderContext( );			context.applicationDomain = ApplicationDomain.currentDomain;			return context;		}	}}