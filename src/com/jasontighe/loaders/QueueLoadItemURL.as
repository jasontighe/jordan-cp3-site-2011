package com.jasontighe.loaders {	import flash.events.Event;	import flash.net.URLLoader;	import flash.net.URLRequest;	import com.jasontighe.loaders.AbstractQueueLoadItem;	import com.jasontighe.loaders.events.QueueLoadItemEvent;		public class QueueLoadItemURL extends AbstractQueueLoadItem	{		public function QueueLoadItemURL( url:String )		{									super( url );								_loadItem = new URLLoader( );			_loadListener = _loadItem;		}				override public function load():void		{						if ( isValidUrl( ) )			{							addListeners( );							_loadItem.load( new URLRequest( _url ) );			}							}			override protected function onQueueLoadItemComplete( event:Event ):void		{			var data:* = URLLoader( event.target ).data;			var bl:uint = URLLoader( _loadListener ).bytesLoaded;			var bt:uint = URLLoader( _loadListener ).bytesTotal;						dispatchEvent( new QueueLoadItemEvent( QueueLoadItemEvent.QUEUE_LOAD_ITEM_COMPLETE, false, false, data, bl, bt ) );			dispatchEvent( new QueueLoadItemEvent( QueueLoadItemEvent.QUEUE_LOAD_ITEM_REMOVE ) );		}		}}