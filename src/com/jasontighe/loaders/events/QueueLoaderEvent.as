package com.jasontighe.loaders.events{	import flash.events.Event;	public class QueueLoaderEvent extends Event 	{		private var _p:Number;		public static const QUEUE_LOADER_START:String = new String( 'queueLoaderStart' );		public static const QUEUE_LOADER_PROGRESS:String = new String( 'queueLoaderProgress' );		public static const QUEUE_LOADER_COMPLETE:String = new String( 'queueLoaderComplete' );		public function QueueLoaderEvent( type:String, bubbles:Boolean = false, cancelable:Boolean = false, p:Number = 0 )		{			super( type, bubbles, cancelable );						_p = p;		}		public function get p():Number		{			return _p;			}	}}