package com.jasontighe.loaders.events{	import flash.events.Event;	public class QueueLoadItemEvent extends Event 	{		private var _loadItem:*;		private var _bl:uint;		private var _bt:uint;		public static const QUEUE_LOAD_ITEM_START:String = new String( 'queueLoadItemStart' );		public static const QUEUE_LOAD_ITEM_PROGRESS:String = new String( 'queueLoadItemProgress' );		public static const QUEUE_LOAD_ITEM_COMPLETE:String = new String( 'queueLoadItemComplete' );				public static const QUEUE_LOAD_ITEM_INIT:String = new String( 'queueLoadItemInit' );				public static const QUEUE_LOAD_ITEM_REMOVE:String = new String( 'queueLoadItemRemove' );				public static const QUEUE_LOAD_ITEM_IO_ERROR:String = new String( 'queueLoadItemIOError' );		public function QueueLoadItemEvent( type:String, bubbles:Boolean = false, cancelable:Boolean = false, loadItem:* = null, bl:uint = 0, bt:uint = 0 )		{			super( type, bubbles, cancelable );						_loadItem = loadItem;			_bl = bl;			_bt = bt;		}		public function get loadItem():*		{			return _loadItem;		}		public function get bl():uint		{			return _bl;		}		public function get bt():uint		{			return _bt;		}	}}