package com.jasontighe.containers.events {	import flash.events.Event;		public class ContainerEvent extends Event 	{				public static const INIT:String = new String( 'initContainer' );		public static const SHOW:String = new String( 'showContainer' );		public static const HIDE:String = new String( 'hideContainer' );		public static const LAST_FRAME:String = new String( 'lastFrameContainer' );		public static const DECONSTRUCT:String = new String( 'deconstructContainer' );				public function ContainerEvent( type:String, bubbles:Boolean = false, cancelable:Boolean = false )		{			super( type, bubbles, cancelable );		}			}}