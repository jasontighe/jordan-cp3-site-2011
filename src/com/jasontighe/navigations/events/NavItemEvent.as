package com.jasontighe.navigations.events {	import flash.events.Event;		public class NavItemEvent extends Event 	{		public static const MOUSE_OUT : String = new String( 'navItemMouseOut' );		public static const MOUSE_OVER : String = new String( 'navItemMouseOver' );		public static const MOUSE_UP : String = new String( 'navItemMouseUp' );		public static const MOUSE_DOWN : String = new String( 'navItemMouseDown' );		public static const CANCEL : String = new String( 'navItemCancel' );		public static const CLICK : String = new String( 'navItemClick' );		public static const ACTIVATE : String = new String( 'navItemActivate' );		public static const DEACTIVATE : String = new String( 'navItemDeactivate' );		public static const ENABLE : String = new String( 'navItemEnable' );		public static const DISABLE : String = new String( 'navItemDisable' );		public function NavItemEvent ( type : String, bubbles : Boolean = false, cancelable : Boolean = false )		{			super( type, bubbles, cancelable );		}		}}