package com.jasontighe.navigations {	public class CheckNav extends Nav 	{		override public function setActiveItem( item:INavItem ):void		{		 						if ( _activeIndex == item.getIndex( ) )			{ 				getItemAt( _activeIndex ).deactivate( );				_activeIndex = -1;			}			else			{				if ( _activeIndex != -1 ) 				{ 					getItemAt( _activeIndex ).deactivate( ); 				}				_activeIndex = item.getIndex( );				getItemAt( _activeIndex ).activate( );							}		}	}}