package com.jasontighe.media.players {	import com.jasontighe.media.objects.IMediaObjectProgressive;	import com.jasontighe.media.players.AbstractMediaPlayer;		public class MediaPlayerProgressive extends AbstractMediaPlayer implements IMediaPlayerProgressive	{				public function MediaPlayerProgressive( media:IMediaObjectProgressive, isPlaying:Boolean = false )		{									super( media, isPlaying );			}		override public function get isReady():Boolean		{			var isReady:Boolean = false;			if ( loadTimeRemaining < duration )			{							isReady = true;						}			//			trace('duration > ' + duration);			//			trace('loadTimeRemaining > ' + loadTimeRemaining);			//			trace('_isReady > ' + _isReady);			return isReady;		}		public function get bytesLoaded():Number		{			return getMedia().bytesLoaded;		}		public function get bytesTotal():Number		{			return getMedia().bytesTotal;		}				public function get loadTimeElapsed():Number		{			return getMedia().loadTimeElapsed;		}		public function get loadTimeRemaining():Number		{			return getMedia().loadTimeRemaining;		}				private function getMedia():IMediaObjectProgressive		{			return IMediaObjectProgressive( _media );		}		}}