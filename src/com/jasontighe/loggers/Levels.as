package com.jasontighe.loggers {	public class Levels 	{		private static const DEBUG_ID:String = "debug";		private static const INFO_ID:String = "info";		private static const WARN_ID:String = "warn";		private static const ERROR_ID:String = "error";		private static const CRITICAL_ID:String = "critical";		public static const DEBUG:LevelObject = new LevelObject( DEBUG_ID, INFO_ID, 0 );		public static const INFO:LevelObject = new LevelObject( INFO_ID, INFO_ID, 1 );		public static const WARN:LevelObject = new LevelObject( WARN_ID, WARN_ID, 2 );		public static const ERROR:LevelObject = new LevelObject( ERROR_ID, ERROR_ID, 3 );		public static const CRITICAL:LevelObject = new LevelObject( CRITICAL_ID, ERROR_ID, 4 );	}}